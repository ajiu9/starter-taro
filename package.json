{
  "name": "starter-taro",
  "version": "1.0.0",
  "description": "This is a taro template",
  "templateInfo": {
    "name": "vue3-pinia",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint .",
    "postinstall": "weapp-tw patch"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "4.0.9",
    "@tarojs/helper": "4.0.9",
    "@tarojs/plugin-framework-vue3": "4.0.9",
    "@tarojs/plugin-platform-alipay": "4.0.9",
    "@tarojs/plugin-platform-h5": "4.0.9",
    "@tarojs/plugin-platform-jd": "4.0.9",
    "@tarojs/plugin-platform-qq": "4.0.9",
    "@tarojs/plugin-platform-swan": "4.0.9",
    "@tarojs/plugin-platform-tt": "4.0.9",
    "@tarojs/plugin-platform-weapp": "4.0.9",
    "@tarojs/runtime": "4.0.9",
    "@tarojs/shared": "4.0.9",
    "@tarojs/taro": "4.0.9",
    "pinia": "^2.0.10",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@ajiu9/eslint-config": "^2.5.3",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@tarojs/cli": "4.0.9",
    "@tarojs/vite-runner": "4.0.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "babel-preset-taro": "4.0.9",
    "eslint": "^9.22.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "sass": "^1.60.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^14.4.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.16.8",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "vite": "^4.2.0",
    "weapp-tailwindcss": "^4.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
